{
  "version": 2,
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    },
    {
      "src": "server/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/client/$1"
    }
  ],
  "env": {
    "JWT_SECRET": "65429ead1189f282322e2a0c90c0de6413c27d144d0c86d6bc4a7c15a266f26f1b6de719422d200bf76664110b3581790df3a931d802757e4f68ed5c862d5268",
    "NODE_ENV": "production"
  },
  "functions": {
    "server/index.js": {
      "maxDuration": 10
    }
  }
}